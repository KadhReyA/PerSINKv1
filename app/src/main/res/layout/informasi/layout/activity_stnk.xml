<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".stnk">

    <TextView
            android:text="@string/judulinputSTNK"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView9" android:textSize="30sp"
            android:textStyle="bold" app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="16dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="167dp"
            android:layout_marginRight="167dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView12" android:text="No. STNK"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="96dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="34dp"
            android:layout_marginStart="34dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editSTNK" android:layout_marginTop="26dp"
            app:layout_constraintTop_toBottomOf="@+id/textView9" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="28dp" android:layout_marginRight="28dp"/>
    <TextView
            android:text="Plat Nomor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView14" android:layout_marginTop="45dp"
            app:layout_constraintTop_toBottomOf="@+id/textView12" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="34dp" android:layout_marginStart="34dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editPlat" android:layout_marginTop="23dp"
            app:layout_constraintTop_toBottomOf="@+id/editSTNK" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="28dp" android:layout_marginRight="28dp"/>
    <TextView
            android:text="Alamat"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView15" android:layout_marginTop="52dp"
            app:layout_constraintTop_toBottomOf="@+id/textView14" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="34dp" android:layout_marginStart="34dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editAlmt" android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/editPlat" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="28dp" android:layout_marginRight="28dp"/>
    <TextView
            android:text="Merk"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView16" android:layout_marginTop="54dp"
            app:layout_constraintTop_toBottomOf="@+id/textView15" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="34dp" android:layout_marginStart="34dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editMerk" android:layout_marginTop="25dp"
            app:layout_constraintTop_toBottomOf="@+id/editAlmt" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="28dp" android:layout_marginRight="28dp"/>
    <TextView
            android:text="Jenis"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView17" android:layout_marginTop="54dp"
            app:layout_constraintTop_toBottomOf="@+id/textView16" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="34dp" android:layout_marginStart="34dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editJenis" android:layout_marginTop="26dp"
            app:layout_constraintTop_toBottomOf="@+id/editMerk" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="28dp" android:layout_marginRight="28dp"/>
    <TextView
            android:text="Tahun"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView18"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="32dp" android:layout_marginStart="32dp" android:layout_marginTop="48dp"
            app:layout_constraintTop_toBottomOf="@+id/textView17"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editTahun" android:layout_marginTop="29dp"
            app:layout_constraintTop_toBottomOf="@+id/editJenis" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="28dp" android:layout_marginRight="28dp"/>
    <TextView
            android:text="Berlaku s/d"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView19" android:layout_marginTop="52dp"
            app:layout_constraintTop_toBottomOf="@+id/textView18" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="32dp" android:layout_marginStart="32dp"/>
    <TextView
            android:text="Tanggal"
            android:layout_width="126dp"
            android:layout_height="30dp"
            android:id="@+id/textview81" android:textSize="18sp"
            android:layout_marginTop="36dp"
            app:layout_constraintTop_toBottomOf="@+id/editTahun" app:layout_constraintStart_toEndOf="@+id/textView19"
            android:layout_marginLeft="66dp" android:layout_marginStart="66dp"/>
    <Button
            android:text="Pilih Tanggal"
            android:layout_width="80dp"
            android:layout_height="55dp"
            android:id="@+id/btnTanggal2"
            android:background="@android:drawable/btn_default_small"
            android:layout_marginTop="36dp" app:layout_constraintTop_toBottomOf="@+id/editTahun"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="28dp"
            android:layout_marginRight="28dp"/>
    <Button
            android:text="Simpan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnSimpanSTNK" android:layout_marginTop="72dp"
            app:layout_constraintTop_toBottomOf="@+id/textView19" app:layout_constraintEnd_toStartOf="@+id/btnHapusSTNK"
            android:layout_marginEnd="100dp" android:layout_marginRight="100dp"/>
    <Button
            android:text="Hapus"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnHapusSTNK" android:layout_marginEnd="64dp" android:layout_marginRight="64dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="36dp" app:layout_constraintTop_toBottomOf="@+id/btnTanggal2"/>
</android.support.constraint.ConstraintLayout>